directory = ###yourdirectoryhere
newdirectory = ##yournewdirectoryhere

for subdir, dirs, files in os.walk(directory):
    for file in files:
        try: 

            img = Image.open(os.path.join(subdir, file))
            new_img = img.resize( (256, 256) )
                
            if str(file) in cancer_patients:
                print(file)
                new_img.save(newdirectory + r'Cancer\\' + file + '.bmp')
                
            elif str(file) in not_cancer:
                new_img.save(newdirectory + r'Not_Cancer\\'+ file + '.bmp')

             

        except TypeError:
            print(str(subdir) + "failed")
                
